{
  "name": "bamimi",
  "version": "0.1.0",
  "description": "Super framework for nodejs",
  "main": "server.js",
  "bin": {
    "bamimi-schedule-cli": "./src/routes/cli/index.js"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "start-dev": "nodemon ./src/server.js",
    "socket": "node ./src/socket.js",
    "socket-dev": "nodemon ./src/socket.js",
    "migration:create": "npx sequelize-cli migration:generate",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:create": "npx sequelize-cli seed:generate",
    "seed:select": "npx sequelize-cli db:seed --seed",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "lint": "npx eslint . ",
    "lint:fix": "npx eslint . --fix",
    "test": "jest --runInBand --force-exit --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:knfs-jsc/bamimi.git"
  },
  "author": "Kent Phung",
  "keywords": [
    "cms",
    "framework"
  ],
  "license": "MIT",
  "dependencies": {
    "@knfs-tech/bamimi-auth": "^1.1.1",
    "@knfs-tech/bamimi-autoload": "^1.0.6",
    "@knfs-tech/bamimi-log": "^1.0.1",
    "@knfs-tech/bamimi-schedule": "^1.0.1",
    "@knfs-tech/bamimi-socket.io": "^1.0.5",
    "@knfs-tech/csrf": "^1.0.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "bullmq": "^5.8.4",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.9",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.18.0",
    "express-validator": "^6.14.2",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.3.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "response-time": "^2.3.2",
    "sequelize": "^6.37.3",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0",
    "socket.io-client": "^4.6.1"
  },
  "autoload": {
    "files": {
      "./src/utils/asset.js": "asset",
      "./src/utils/url.js": "url",
      "./src/utils/file.js": "file",
      "./src/utils/csrf.js": "csrf",
      "./src/utils/date.js": "date"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^7.6.0",
    "@knfs-tech/eslint-plugin": "^1.9.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-register": "^6.26.0",
    "eslint": "^9.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-naming-convention": "^0.1.3",
    "globals": "^15.8.0",
    "jest": "^29.4.3",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "typescript": "^5.5.4"
  },
  "private": true
}